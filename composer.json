{
  "name": "plancke/hypixel-php",
  "description": "A PHP wrapper for the Hypixel Public API",
  "homepage": "https://plancke.io",
  "license": "MIT",
  "authors": [
    {
      "name": "Plancke",
      "email": "plancke@hypixel.net",
      "role": "Developer"
    }
  ],
  "keywords": [
    "hypixel"
  ],
  "support": {
    "issues": "https://github.com/Plancke/hypixel-php/issues"
  },
  "require": {
    "php": "^7.0",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "suggest": {
    "mongodb/mongodb": "^1.0.0"
  },
  "autoload": {
    "psr-4": {
      "Plancke\\HypixelPHP\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "5.5.*"
  },
  "autoload-dev": {
    "psr-4": {
      "Plancke\\Tests\\": "tests/"
    }
  }
}
